<h5> User Profiles </h5>


<% @users_all.each do |user| %>     
    <div class="row">
            <div class="card horizontal white" style="width:50%">
                <!--
                <div class="card-image">
                    <img src="http://www.ourbloc.co/uploads/2/4/0/1/24014742/coverpic_orig.jpg">
                  </div> -->
            <div class="card-content teal-text">
            <h5><%= user.name %> </h5> 
           <h6><%= user.email %> </h6>
           <h6><%= user.prep %> </h6>
            <h6><%= user.firstjob %> </h6>
            <h6><%= user.style %> </h6>
            <h6><%= user.grad %> </h6>


              
              
              <% if user_signed_in? and user.role == "company" %> 
                  <h6><%= user.email %></h6>
                <% end %> 
                
        
         </div>
        </div> 
        
        
          <% end %>
 