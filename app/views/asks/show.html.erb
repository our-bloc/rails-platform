<p id="notice"><%= notice %></p>

<div class="row">
  <div class="col s12 m6">
    <div class="card grey darken-2">
      <div class="card-content white-text">
        <span class="card-title"><%= @ask.title %>, <%= @ask.sector %></span>
        <p><%= @ask.body %></p>
      </div>
      <div class="card-action">
        <a href="#"><%= link_to 'Edit', edit_ask_path(@ask) %></a>
        <a href="#"><%= link_to 'Back', asks_path %></a>
      </div>
    </div>
  </div>
</div>
