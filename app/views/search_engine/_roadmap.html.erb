
  
 <!-- Compiled and minified CSS -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">

  <!-- Compiled and minified JavaScript -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>
          

  <div class="web-sidebar" > 
      <div class="left-menu">
        <img class="circle" id="logo" src="data:image/jpeg;base64, ">
        <a href=#resources> <p style="margin-top: 10%"> YOUR RESOURCES </p></a>
        <p style="margin-top:10%; font-size: 1.5em"> <%= link_to "YOUR RESUME TEMPLATE" , resumebuilder_path%> </p>
        <a href=#opportunities><p style="margin-top: 10%"> OPPORTUNITIES </p> </a> 
        <a href=#rolemodels><p style="margin-top: 10%"> ROLE MODELS </p> </a> 
        
        <div style="margin-left: 3em">
          <% if user_signed_in? %> 
            <%= current_user.name %> 
            <%= link_to "my account", myprofile_path , class: "cyan-text" %> 
          <% end %> 
        </div> 
      
      </div>
      
  </div>
  
  <div class="row"> 
  <div class="col s3"></div>
  <div class="col s9"> 
        <div class="bgdiv" style="z-index: -3"> 
            <img class="sidepic" src="img/bgimages.jpg"> 
            
         
          
            
         </div>
         
         <section id="menu" style="z-index: 5"> 
                   <a href=#resources> <h5 style="margin-top: 10%"> WELCOME TO YOUR PERSONAL CAREER PORTAL </h5> </a>
                   
                   <p class="white-text" style="margin-left: 0%"> " Making your mark on the world is hard. If it were easy, everybody would do it. But it's not. 
                   It takes patience, it takes commitment and it comes with plenty of failure along the way. "</p>
                   
                   <p class="white-text" style="margin-left: 50%"> Barack Obama </p> 
                   
            </section>
        
        <section id="resume" style="background-color: white"> 
          
            <%= render "search_engine/resumebuilder" %> 
        </section>  
        
        
        
        <section id="resources"> 
          <div class="row">
          <% @tips[0...2].each do |tip| %> 
                <div class="col s12 l6">
                  <div class="card" style="40em">
                    <div class="card-image">
                      <img src="https://images.pexels.com/photos/47224/pexels-photo-47224.jpeg?h=350&auto=compress&cs=tinysrgb">
                      <span class="card-title white-text cyan" style="font-weight: 800; height: 5em ; text-transform:uppercase; font-size:1.5em "> <%= tip.title %> </span>
                    </div>
                    <div class="card-content">
                      <p style="font-size: .7em"><%= tip.body %> </p>
                    </div>
                <div class="card-action" style="height: 2em; position:absolute;right: 2em; bottom: 2em">

                      <div class="votes teal-text tip_like" style="position:absolute; bottom:2em; right: 5em" > 
                        <% if tip.liked_by @user %> 
                          <%= link_to '<i class="fa fa-heart-o fa-2x "></i>' .html_safe, up_votes_path(object:'Tip', id: 5)  ,method: :get, remote: true , class: "like_object", :style => "color:#4dd0e1"%> 
                        <% else %> 
                          <%= link_to '<i class="fa fa-heart fa-2x "></i>' .html_safe, down_votes_path(object:'Tip', id: 5) ,method: :get, remote: true, class: "unlike_object",:style => "color:#4dd0e1" %>
                        <% end %> 
                        </div>
                    </div>
                  </div>
                </div>
                <% end %> 
                
              </div>
        
          
        </section> 
              
          
              
              
        <section id="opportunities">    
          <h5> YOUR OPPORTUNITIES </h5>
        
          <%  @jobs[0...2].each do |job| %>
              
                <p> <i class="fa fa-star yellow-text"> </i><%= link_to job.title + " @ " + job.company , job_path(job.id)  , :class => "white-text lighten-1 opp_links" %> 
  
            <% end %> 
          <%  @indeed_results[0...5].each do |job| %>
              
                <p><%= link_to job.job_title + " @ " + job.company , job.url  , :class => "white-text lighten-1 opp_links" %> 
  
            <% end %> 
            
             
        </section>
        
        <section id="more_advice">
          <h5> MORE CAREER ADVICE ... </h5>
        
            <%  @musedata[0...5].each do |tip| %> 
                  <p style="color:white"> <%= link_to tip["name"] , "https://www.themuse.com/advice/"+ tip["short_name"].to_s , :style => "color:#4dd0e1" %> </p>
                          
        
                <% end %> 
      </section>   
      
      <section id="rolemodels">
          <div class="row">
          <% @influencers[0...2].each do |influencer| %> 
                <div class="col s12 l6">
                  <div class="card" style="height: 40em">
                    <div class="card-image">
                      <img src="<%= influencer.image%> ">
                      <span class="card-title white-tText" style="font-weight: 800"> </span>
                    </div>
                    <div class="card-content">
                      <p style="font-size: .7em"><%= influencer.description.truncate(500) %> </p>
                    </div>
                    <div class="card-action" style="height: 2em; position:absolute;right: 2em; bottom: 2em">
                      <div class="votes teal-text tip_like" > 
                        <% if influencer.liked_by @user %> 
                          <%= link_to '<i class="fa fa-heart-o fa-2x"></i>' .html_safe, up_votes_path(object:'Influencer', id: 2)  ,method: :get, remote: true , class: "like_object", :style => "color:#4dd0e1"%> 
                        <% else %> 
                          <%= link_to '<i class="fa fa-heart fa-2x"></i>' .html_safe, down_votes_path(object:'Influencer', id: 2) ,method: :get, remote: true, class: "unlike_object" , :style => "color:#4dd0e1"%>
                        <% end %> 
                        </div>
                    </div>
                  </div>
                </div>
                <% end %> 
                
              </div>
        
        
      </section>
               
    </div>
</div>
       
<style>
   .sidepic{
     position:absolute;
     top: 0;
     left: 0%;
     overflow: hidden;
     width: 100%;
     height: 100%;
     opacity: 1;
     }
 
   .bgdiv{
     position:fixed;
     top: 0;
     left: 30%;
     height: 100%;
     width: 100%;
   }
   
   h5{ 
     color: white;
   }
  
    #opportunities a:hover{
      background-color: inherit;
    }
    
    #menu h5, #opportunities h5, #opportunities p {
      color: white;
    }
    
    .opp_links{
      font-weight: 800;
    }
    
    .left-menu{
      padding: 0%;
      margin:0%;
      left: -10em;
      position:absolute;
    }
    
    .web-sidebar{
      position: fixed;
      padding:0%;
    }
    
    #logo{
      width: 45%;
    }
    .opp_links:hover {
      color: yellow !important;
    }
</style>

<script> 
$( document ).ready(function() {
    $('body').css('overflow','hidden');
    $('body').attr('scroll','no');
    $('.hide_job_email').hide();
});
</script>