<p id="notice"><%= notice %></p>

<h1>Tips</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>Industry</th>
      <th>Link</th>
      <th>Prep</th>
      <th>Grad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tips.each do |tip| %>
      <tr>
        <td><%= tip.title %></td>
        <td><%= tip.body %></td>
        <td><%= tip.industry %></td>
        <td><%= tip.link %></td>
        <td><%= tip.prep %></td>
        <td><%= tip.grad %></td>
        <td><%= link_to 'Show', tip %></td>
        <td><%= link_to 'Edit', edit_tip_path(tip) %></td>
        <td><%= link_to 'Destroy', tip, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tip', new_tip_path %>
