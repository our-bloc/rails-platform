<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Org</th>
      <th>Date</th>
      <th>User</th>
      <th>Contact</th>
      <th>Industry</th>
      <th>Body</th>
      <th>Social</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.org %></td>
        <td><%= event.date %></td>
        <td><%= event.user_id %></td>
        <td><%= event.contact %></td>
        <td><%= event.industry %></td>
        <td><%= event.body %></td>
        <td><%= event.social %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
